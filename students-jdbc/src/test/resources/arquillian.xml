<arquillian
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jboss.org/schema/arquillian"
	xsi:schemaLocation="https://jboss.org/schema/arquillian
                https://jboss.org/schema/arquillian/arquillian_1_0.xsd">

	<container qualifier="payara" default="true">
		<configuration>
			<property name="bindHttpPort">8081</property>
			<property name="bindHttpsPort">8443</property>
		</configuration>
	</container>
</arquillian>
